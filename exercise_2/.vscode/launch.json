{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Assembly Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test_swapcase",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for GDB",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set disassembly flavor to AT&T",
                    "text": "set disassembly-flavor att",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "build1"
        }
    ]
}
